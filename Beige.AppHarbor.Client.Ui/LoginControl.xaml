<UserControl x:Class="Beige.AppHarbor.Client.Ui.LoginControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="122" d:DesignWidth="354">
    <Grid Width="349" Height="124">
        <Label Content="Username" Height="28" HorizontalAlignment="Left" Margin="12,12,0,0" Name="label1" VerticalAlignment="Top" />
        <Label Content="Password" Height="28" HorizontalAlignment="Left" Margin="12,46,0,0" Name="label2" VerticalAlignment="Top" />
        <PasswordBox Height="23" HorizontalAlignment="Left" Margin="74,46,0,0" Name="txtPassword" VerticalAlignment="Top" Width="248" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="74,17,0,0" Name="txtUsername" VerticalAlignment="Top" Width="248" />
        <Button Content="Login" Height="23" HorizontalAlignment="Left" Margin="166,84,0,0" Name="btnLogin" VerticalAlignment="Top" Width="75" />
        <Button Content="Cancel" Height="23" HorizontalAlignment="Right" Margin="0,84,27,0" Name="btnCancel" VerticalAlignment="Top" Width="75" />
    </Grid>
</UserControl>
